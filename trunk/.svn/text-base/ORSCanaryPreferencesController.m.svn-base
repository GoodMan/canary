#import "ORSCanaryPreferencesController.h"

@implementation ORSCanaryPreferencesController

- (IBAction) timelineRefreshRateSelected:sender {
	[[CanaryController sharedController] updateTimer];
	NSLog(@"ORSCanaryPreferencesController:: timelineRefreshRateSelected");
}

- (IBAction) maxShownUpdatesSelected:sender {
	[[CanaryController sharedController] updateMaxNoOfShownUpdates];
	NSLog(@"ORSCanaryPreferencesController:: maxShownUpdatesSelected");
}

- (IBAction) urlShortenerSelected:sender {
	[[CanaryController sharedController] updateSelectedURLShortener];
	NSLog(@"ORSCanaryPreferencesController:: urlShortenerSelected");
}

@end
